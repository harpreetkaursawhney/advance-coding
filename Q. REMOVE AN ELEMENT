#include <stdio.h>

int removeElement(int arr[], int size, int key) {
    int i, j = 0;
    for (i = 0; i < size; i++) {
        if (arr[i] != key) {
            arr[j++] = arr[i];
        }
    }
    return j; // New length
}

int main() {
    int arr[] = {1, 2, 3, 2, 4, 2, 5};
    int key = 2;
    int size = sizeof(arr) / sizeof(arr[0]);

    int newSize = removeElement(arr, size, key);
    printf("Array after removing %d:\n", key);
    for (int i = 0; i < newSize; i++) {
        printf("%d ", arr[i]);
    }
    printf("\nNumber of elements not equal to %d: %d\n", key, newSize);
    return 0;
}
